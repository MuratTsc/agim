<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Zümre Akademik Toplantı Kurulu</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->


<div class="row">

    <% if (mesaj == "no") { %>
    <% } else { %>
    <div class="alert alert-success">
    <strong><%= mesaj %></strong> 
    </div>
    <% } %>

</div>

<div class="row">
    <h3>Zümre Rapor Listesi</h3>
    <table width="100%" id="dataTable" class="table table-striped table-bordered table-hover">

        <thead>
            <tr>
                <th>Dönem</th>
                <th>Rapor No</th>
                <th>Zümre Başkanı</th>
                <th>Akademik Danışman</th>
                <th>Tarih</th>
            </tr>
        </thead>

        <tbody>
        
            <% for (var i = 0; i < zumreRaporlar.length; i++) { %>
                <tr onclick="document.location = '/zumreRaporGoruntuleme/<%= zumreRaporlar[i].id %>';" style="cursor: pointer;">
                <td><%= zumreRaporlar[i].donem %></td>
                <td><%= zumreRaporlar[i].raporNo %></td>
                <td><%= zumreRaporlar[i].zumreBaskani %></td>
                <td><%= zumreRaporlar[i].akademikDanisman %></td>
                <td><%= zumreRaporlar[i].raporTarih.getDate() %>.<%= zumreRaporlar[i].raporTarih.getMonth()+1 %>.<%= zumreRaporlar[i].raporTarih.getFullYear() %></td>
                
                </tr>
            <% } %>
        
        </tbody>

    </table>


</div>
<!-- /.row -->