
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title><%= main.head.title %></title>
  <meta content="width=device-width, initial-scale=1.0" name="viewport">
  <meta content="<%= main.head.meta.keywords %>" name="keywords">
  <meta content="<%= main.head.meta.description %>" name="description">

  <!-- Favicons 
  <link href="img/favicon.png" rel="icon">
  <link href="img/apple-touch-icon.png" rel="apple-touch-icon">
  -->

  <!-- Google Fonts -->
  <link href="/public/dist/css/font-css-opensans-montserrat.css" rel="stylesheet">

  <!-- Bootstrap CSS File 
  <link href="public/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

  <!-- Libraries CSS Files 
  <link href="public/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet">
  -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css" rel="stylesheet">
  <link href="/public/dist/css/animate.min.css" rel="stylesheet">
  <link href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.0/css/ionicons.min.css" rel="stylesheet">
  <link href="/public/dist/css/owl.carousel.min.css" rel="stylesheet">
  <link href="/public/dist/css/lightbox.min.css" rel="stylesheet">

  <!-- Main Stylesheet File -->
  <link href="/public/dist/css/style.css" rel="stylesheet">

</head>

<body>

  <!--==========================
    Header
  ============================-->
  <header id="header">
    <div class="container-fluid">

      <div id="logo" class="pull-left">
        <h1><a href="#intro" class="scrollto"><%= main.body.header.logo.title %></a></h1>
        <!-- Uncomment below if you prefer to use an image logo -->
        <!-- <a href="#intro"><img src="img/logo.png" alt="" title="" /></a>-->
      </div>

      <nav id="nav-menu-container">
        <ul class="nav-menu">
          <li class="menu-active"><a href="#intro"><%= main.body.header.menu.item1 %></a></li>
          <% if (main.body.main.about.isActive) { %>
          <li><a href="#about"><%= main.body.header.menu.item2 %></a></li>
          <% } %>
          <% if (main.body.main.services.isActive) { %>
          <li><a href="#services"><%= main.body.header.menu.item3 %></a></li>
          <% } %>
          <% if (main.body.main.facts.isActive) { %>
          <li><a href="#facts"><%= main.body.header.menu.item4 %></a></li>
          <% } %>
          <!-- 
          <li><a href="#team">EKİP</a></li>
          <li class="menu-has-children"><a href="">Drop Down</a>
            <ul>
              <li><a href="#">Drop Down 1</a></li>
              <li><a href="#">Drop Down 3</a></li>
              <li><a href="#">Drop Down 4</a></li>
              <li><a href="#">Drop Down 5</a></li>
            </ul>
          </li>
          -->
          <% if (main.body.main.contact.isActive) { %>
          <li><a href="#contact"><%= main.body.header.menu.item5 %></a></li>
          <% } %>
        </ul>
      </nav><!-- #nav-menu-container -->
    </div>
  </header><!-- #header -->

  <!--==========================
    Intro Section
  ============================-->
  <section id="intro">
    <div class="intro-container">
    
      <div id="introCarousel" class="carousel  slide carousel-fade" data-ride="carousel">

        <ol class="carousel-indicators"></ol>

        <div class="carousel-inner" role="listbox">

          <div class="carousel-item active">
            <div class="carousel-background"><img src="<%= main.body.intro.slide1.img %>" alt=""></div>
            <div class="carousel-container">
              <div class="carousel-content">
                <h2><%= main.body.intro.slide1.title %></h2>
                <p><%= main.body.intro.slide1.description %></p>
                <a href="<%= main.body.intro.slide1.button.href %>" class="btn-get-started scrollto"><%= main.body.intro.slide1.button.title %></a>
              </div>
            </div>
          </div>

        <% if (main.body.intro.slide2.isActive) { %>
          <div class="carousel-item">
            <div class="carousel-background"><img src="<%= main.body.intro.slide2.img %>" alt=""></div>
            <div class="carousel-container">
              <div class="carousel-content">
                <h2><%= main.body.intro.slide2.title %></h2>
                <p><%= main.body.intro.slide2.description %></p>
                <a href="<%= main.body.intro.slide2.button.href %>" class="btn-get-started scrollto"><%= main.body.intro.slide2.button.title %></a>
              </div>
            </div>
          </div>
        <% } %>

        <% if (main.body.intro.slide3.isActive) { %>
          <div class="carousel-item">
            <div class="carousel-background"><img src="<%= main.body.intro.slide3.img %>" alt=""></div>
            <div class="carousel-container">
              <div class="carousel-content">
                <h2><%= main.body.intro.slide3.title %></h2>
                <p><%= main.body.intro.slide3.description %></p>
                <a href="<%= main.body.intro.slide3.button.href %>" class="btn-get-started scrollto"><%= main.body.intro.slide3.button.title %></a>
              </div>
            </div>
          </div>
        <% } %>

        <% if (main.body.intro.slide4.isActive) { %>
          <div class="carousel-item">
            <div class="carousel-background"><img src="<%= main.body.intro.slide4.img %>" alt=""></div>
            <div class="carousel-container">
              <div class="carousel-content">
                <h2><%= main.body.intro.slide4.title %></h2>
                <p><%= main.body.intro.slide4.description %></p>
                <a href="<%= main.body.intro.slide4.button.href %>" class="btn-get-started scrollto"><%= main.body.intro.slide4.button.title %></a>
              </div>
            </div>
          </div>
        <% } %>

        <% if (main.body.intro.slide5.isActive) { %>
          <div class="carousel-item">
            <div class="carousel-background"><img src="<%= main.body.intro.slide5.img %>" alt=""></div>
            <div class="carousel-container">
              <div class="carousel-content">
                <h2><%= main.body.intro.slide5.title %></h2>
                <p><%= main.body.intro.slide5.description %></p>
                <a href="<%= main.body.intro.slide5.button.href %>" class="btn-get-started scrollto"><%= main.body.intro.slide5.button.title %></a>
              </div>
            </div>
          </div>
        <% } %>

        </div>

        <a class="carousel-control-prev" href="#introCarousel" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon ion-chevron-left" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>

        <a class="carousel-control-next" href="#introCarousel" role="button" data-slide="next">
          <span class="carousel-control-next-icon ion-chevron-right" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>

      </div>-
    </div>
  </section><!-- #intro -->

  <main id="main">

    <!--==========================
      Featured Services Section
    ============================-->
    <% if (main.body.main.featured.isActive) { %>
    <section id="featured-services">
      <div class="container">
        <div class="row">

          <div class="col-lg-4 box">
            <i class="<%= main.body.main.featured.item1.icon %>"></i>
            <h4 class="title"><a href=""><%= main.body.main.featured.item1.title %></a></h4>
            <p class="description"><%= main.body.main.featured.item1.description %></p>
          </div>

          <div class="col-lg-4 box box-bg">
            <i class="<%= main.body.main.featured.item2.icon %>"></i>
            <h4 class="title"><a href=""><%= main.body.main.featured.item2.title %></a></h4>
            <p class="description"><%= main.body.main.featured.item2.description %></p>
          </div>

          <div class="col-lg-4 box">
            <i class="<%= main.body.main.featured.item3.icon %>"></i>
            <h4 class="title"><a href=""><%= main.body.main.featured.item3.title %></a></h4>
            <p class="description"><%= main.body.main.featured.item3.description %></p>
          </div>

        </div>
      </div>
    </section><!-- #featured-services -->
    <% } %>
    <!--==========================
      About Us Section
    ============================-->
    <% if (main.body.main.about.isActive) { %>
    <section id="about">
      <div class="container">

        <header class="section-header">
          <h3><%= main.body.main.about.header.title %></h3>
          <p><%= main.body.main.about.header.description %></p>
        </header>

        <div class="row about-cols">

          <div class="col-md-4 wow fadeInUp">
            <div class="about-col">
              <div class="img">
                <img src="<%= main.body.main.about.item1.img.src %>" alt="<%= main.body.main.about.item1.img.alt %>" class="img-fluid">
                <div class="icon"><i class="<%= main.body.main.about.item1.icon %>"></i></div>
              </div>
              <h2 class="title"><a href="#"><%= main.body.main.about.item1.title %></a></h2>
              <p>
                <%= main.body.main.about.item1.description %>
              </p>
            </div>
          </div>

          <div class="col-md-4 wow fadeInUp" data-wow-delay="0.1s">
            <div class="about-col">
              <div class="img">
                <img src="<%= main.body.main.about.item2.img.src %>" alt="" class="img-fluid">
                <div class="icon"><i class="<%= main.body.main.about.item2.icon %>"></i></div>
              </div>
              <h2 class="title"><a href="#"><%= main.body.main.about.item2.title %></a></h2>
              <p>
                <%= main.body.main.about.item2.description %>
              </p>
            </div>
          </div>

          <div class="col-md-4 wow fadeInUp" data-wow-delay="0.2s">
            <div class="about-col">
              <div class="img">
                <img src="<%= main.body.main.about.item3.img.src %>" alt="" class="img-fluid">
                <div class="icon"><i class="<%= main.body.main.about.item3.icon %>"></i></div>
              </div>
              <h2 class="title"><a href="#"><%= main.body.main.about.item3.title %></a></h2>
              <p>
                <%= main.body.main.about.item3.description %>
              </p>
            </div>
          </div>

        </div>

      </div>
    </section><!-- #about -->
    <% } %>

    <!--==========================
      Services Section
    ============================-->
    <% if (main.body.main.services.isActive) { %>
    <section id="services">
      <div class="container">

        <header class="section-header wow fadeInUp">
          <h3><%= main.body.main.services.header.title %></h3>
          <p><%= main.body.main.services.header.description %></p>
        </header>

        <div class="row">

          <div class="col-lg-4 col-md-6 box wow bounceInUp" data-wow-duration="1.4s">
            <div class="icon"><i class="<%= main.body.main.services.item1.icon %>"></i></div>
            <h4 class="title"><a href=""><%= main.body.main.services.item1.title %></a></h4>
            <p class="description"><%= main.body.main.services.item1.description %></p>
          </div>
          <div class="col-lg-4 col-md-6 box wow bounceInUp" data-wow-duration="1.4s">
            <div class="icon"><i class="<%= main.body.main.services.item2.icon %>"></i></div>
            <h4 class="title"><a href=""><%= main.body.main.services.item2.title %></a></h4>
            <p class="description"><%= main.body.main.services.item2.description %></p>
          </div>
          <div class="col-lg-4 col-md-6 box wow bounceInUp" data-wow-duration="1.4s">
            <div class="icon"><i class="<%= main.body.main.services.item3.icon %>"></i></div>
            <h4 class="title"><a href=""><%= main.body.main.services.item3.title %></a></h4>
            <p class="description"><%= main.body.main.services.item3.description %></p>
          </div>
          <div class="col-lg-4 col-md-6 box wow bounceInUp" data-wow-delay="0.1s" data-wow-duration="1.4s">
            <div class="icon"><i class="<%= main.body.main.services.item4.icon %>"></i></div>
            <h4 class="title"><a href=""><%= main.body.main.services.item4.title %></a></h4>
            <p class="description"><%= main.body.main.services.item4.description %></p>
          </div>
          <div class="col-lg-4 col-md-6 box wow bounceInUp" data-wow-delay="0.1s" data-wow-duration="1.4s">
            <div class="icon"><i class="<%= main.body.main.services.item5.icon %>"></i></div>
            <h4 class="title"><a href=""><%= main.body.main.services.item5.title %></a></h4>
            <p class="description"><%= main.body.main.services.item5.description %></p>
          </div>
          <div class="col-lg-4 col-md-6 box wow bounceInUp" data-wow-delay="0.1s" data-wow-duration="1.4s">
            <div class="icon"><i class="<%= main.body.main.services.item6.icon %>"></i></div>
            <h4 class="title"><a href=""><%= main.body.main.services.item6.title %></a></h4>
            <p class="description"><%= main.body.main.services.item6.description %></p>
          </div>

        </div>

      </div>
    </section><!-- #services -->
    <% } %>

    <!--==========================
      Call To Action Section
    ============================-->
    <% if (main.body.main.callToAction.isActive) { %>
    <section id="call-to-action" class="wow fadeIn">
      <div class="container text-center">
        <h3><%= main.body.main.callToAction.title %></h3>
        <p><%= main.body.main.callToAction.description %></p>
        <a class="cta-btn" href="<%= main.body.main.callToAction.button.href %>"><%= main.body.main.callToAction.button.title %></a>
      </div>
    </section><!-- #call-to-action -->
    <% } %>

    <!--==========================
      Facts Section
    ============================-->
    <% if (main.body.main.facts.isActive) { %>
    <section id="facts"  class="wow fadeIn">
      <div class="container">

        <header class="section-header">
          <h3><%= main.body.main.facts.title %></h3>
          <p><%= main.body.main.facts.description %></p>
        </header>

        <div class="row counters">

  				<div class="col-lg-3 col-6 text-center">
            <span data-toggle="counter-up"><%= istatistik.kurumSayisi %></span>
            <p>Kurum</p>
  				</div>

          <div class="col-lg-3 col-6 text-center">
            <span data-toggle="counter-up"><%= istatistik.kullaniciSayisi %></span>
            <p>Kullanıcı</p>
  				</div>

          <div class="col-lg-3 col-6 text-center">
            <span data-toggle="counter-up"><%= istatistik.materyalSayisi %></span>
            <p>Materyal</p>
  				</div>

          <div class="col-lg-3 col-6 text-center">
            <span data-toggle="counter-up"><%= istatistik.soruSayisi %></span>
            <p>Soru</p>
  				</div>

  			</div>

        <div class="facts-img">
          <img src="<%= main.body.main.facts.img.src %>" alt="" class="img-fluid">
        </div>

      </div>
    </section><!-- #facts -->
    <% } %>

    <!--==========================
      Clients Section
    ============================-->
    <% if (main.body.main.clients.isActive) { %>
    <section id="clients" class="wow fadeInUp">
      <div class="container">

        <header class="section-header">
          <h3><%= main.body.main.clients.title %></h3>
          <p><%= main.body.main.clients.description %></p>
        </header>

        <div class="owl-carousel clients-carousel">
          <% kurumLogo.forEach(logo => { %>
          <img src="<%= logo.logo %>" style="max-height: 80px" alt="">
          <% }); %>
        </div>

      </div>
    </section><!-- #clients -->
    <% } %>

    <!--==========================
      Contact Section
    ============================-->
    <% if (main.body.main.contact.isActive) { %>
    <section id="contact" class="section-bg wow fadeInUp">
      <div class="container">

        <div class="section-header">
          <h3><%= main.body.main.contact.header.title %></h3>
          <p><%= main.body.main.contact.header.description %></p>
        </div>

        <div class="row contact-info">

          <div class="col-md-4">
            <div class="contact-address">
              <i class="<%= main.body.main.contact.info.address.icon %>"></i>
              <h3><%= main.body.main.contact.info.address.title %></h3>
              <address><a href="<%= main.body.main.contact.info.address.descHref %>"><%= main.body.main.contact.info.address.description %></a></address>
            </div>
          </div>

          <div class="col-md-4">
            <div class="contact-phone">
              <i class="<%= main.body.main.contact.info.phone.icon %>"></i>
              <h3><%= main.body.main.contact.info.phone.title %></h3>
              <p><a href="<%= main.body.main.contact.info.phone.descHref %>"><%= main.body.main.contact.info.phone.description %></a></p>
            </div>
          </div>

          <div class="col-md-4">
            <div class="contact-email">
              <i class="<%= main.body.main.contact.info.email.icon %>"></i>
              <h3><%= main.body.main.contact.info.email.title %></h3>
              <p><a href="<%= main.body.main.contact.info.email.descHref %>"><%= main.body.main.contact.info.email.description %></a></p>
            </div>
          </div>

        </div>

        <!-- div class="form">
          <div id="sendmessage">Your message has been sent. Thank you!</div>
          <div id="errormessage"></div>
          <form action="" method="post" role="form" class="contactForm">
            <div class="form-row">
              <div class="form-group col-md-6">
                <input type="text" name="name" class="form-control" id="name" placeholder="Your Name" data-rule="minlen:4" data-msg="Please enter at least 4 chars" />
