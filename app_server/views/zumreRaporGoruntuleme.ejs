<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Zümre Kurul Raporu</h1>
    </div>
    <!-- /.col-lg-12 -->

</div>
<!-- /.row -->


<div class="row">
    <% if (mesaj == "no") { %>
    <% } else { %>
    <div class="alert alert-danger">
        <%- mesaj %>
    </div>
    <% } %>
</div>


<div class="panel panel-info">
    <div class="panel-heading">
        <div class="row">
            <div class="col-xs-12 col-md-4">
                <strong>Dönem:</strong> <%= zumreRapor.donem %>
            </div>
            <div class="col-xs-12 col-md-4">
                <strong>Rapor No:</strong> <%= zumreRapor.raporNo %>
            </div>
            <div class="col-xs-12 col-md-4">
                <strong>Eklenme Tarihi:</strong> <%= zumreRapor.raporTarih.getDate() %>.<%= zumreRapor.raporTarih.getMonth()+1 %>.<%= zumreRapor.raporTarih.getFullYear() %>
            </div>
            <div class="col-xs-12 col-md-4">
                <strong>Zümre Başkanı:</strong> <%= zumreRapor.zumreBaskani %>
            </div>
            <div class="col-xs-12 col-md-4">
                <strong>Akademik Danışman:</strong> <%= zumreRapor.akademikDanisman %>
            </div>
        </div>
    </div>
    <div class="panel-body">
        <%- zumreRapor.raporIcerik %>
    </div>
    <div class="panel-footer noprint">
        <div class="row">
            <div class="col-xs-6 col-md-9">
                &nbsp;
            </div>
            <div class="col-xs-6 col-md-3">
                <% if(butonlar) { %>
                <form action="../zumreRaporSil" method="POST" style="margin:0px;">
                    <input type="text" class="hidden" id="zumreRaporId" name="zumreRaporId" value="<%= zumreRapor.id %>">
                    <button type="submit" class="btn btn-danger">Sil</button>
                    <a href="/zumreKurul/<%=zumreRapor.id%>" class="btn btn-info">Düzenle</a>
                </form>
                <% } %>
            </div>
        </div>
    </div>
</div>


<br><br>