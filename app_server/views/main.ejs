              </div>
              <div class="form-group col-md-6">
                <input type="email" class="form-control" name="email" id="email" placeholder="Your Email" data-rule="email" data-msg="Please enter a valid email" />
                <div class="validation"></div>
              </div>
            </div>
            <div class="form-group">
              <input type="text" class="form-control" name="subject" id="subject" placeholder="Subject" data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject" />
              <div class="validation"></div>
            </div>
            <div class="form-group">
              <textarea class="form-control" name="message" rows="5" data-rule="required" data-msg="Please write something for us" placeholder="Message"></textarea>
              <div class="validation"></div>
            </div>
            <div class="text-center"><button type="submit">Send Message</button></div>
          </form>
        </div -->

      </div>
    </section><!-- #contact -->
    <% } %>

  </main>

  <!--==========================
    Footer
  ============================-->
  <footer id="footer">
    <% if (main.body.footer.isActive) { %>
    <div class="footer-top">
      <div class="container">
        <div class="row">

          <div class="col-lg-3 col-md-6 footer-info">
            <h3><%- main.body.footer.info.title %></h3>
            <p><%- main.body.footer.info.description %></p>
          </div>

          <div class="col-lg-3 col-md-6 footer-links">
            <h4><%- main.body.footer.links.title %></h4>
            <ul>
              <li><i class="ion-ios-arrow-right"></i> <a href="<%= main.body.footer.links.item1.href %>" class="scrollto"><%- main.body.footer.links.item1.title %></a></li>
              <li><i class="ion-ios-arrow-right"></i> <a href="<%= main.body.footer.links.item2.href %>" class="scrollto"><%- main.body.footer.links.item2.title %></a></li>
              <li><i class="ion-ios-arrow-right"></i> <a href="<%= main.body.footer.links.item3.href %>" class="scrollto"><%- main.body.footer.links.item3.title %></a></li>
              <li><i class="ion-ios-arrow-right"></i> <a href="<%= main.body.footer.links.item4.href %>" class="scrollto"><%- main.body.footer.links.item4.title %></a></li>
              <li><i class="ion-ios-arrow-right"></i> <a href="<%= main.body.footer.links.item5.href %>" class="scrollto"><%- main.body.footer.links.item5.title %></a></li>
            </ul>
          </div>

          <div class="col-lg-3 col-md-6 footer-contact">
            <h4><%- main.body.footer.contact.title %></h4>
            <p>
              <%- main.body.footer.contact.description %>
            </p>

            <div class="social-links">
              <a href="<%= main.body.footer.contact.social.twitter %>" class="twitter"><i class="fa fa-twitter"></i></a>
              <a href="<%= main.body.footer.contact.social.facebook %>" class="facebook"><i class="fa fa-facebook"></i></a>
              <a href="<%= main.body.footer.contact.social.instagram %>" class="instagram"><i class="fa fa-instagram"></i></a>
              <a href="<%= main.body.footer.contact.social.googleplus %>" class="google-plus"><i class="fa fa-google-plus"></i></a>
              <a href="<%= main.body.footer.contact.social.linkedin %>" class="linkedin"><i class="fa fa-linkedin"></i></a>
            </div>

          </div>

          <div class="col-lg-3 col-md-6 footer-newsletter">
            <h4><%- main.body.footer.newsletter.title %></h4>
            <p><%- main.body.footer.newsletter.description %></p>
            <!-- form action="" method="post">
              <input type="email" name="email"><input type="submit"  value="Subscribe">
            </form -->
          </div>

        </div>
      </div>
    </div>
    <% } %>

    <div class="container">
      <div class="copyright">
        <%- main.body.footer.copyright %>
      </div>
      <div class="credits">
        <%- main.body.footer.credits %>
      </div>
    </div>
  </footer><!-- #footer -->

  <a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>

  <!-- JavaScript Libraries -->
  <script src="/public/dist/lib/jquery/jquery.min.js"></script>
  <script src="/public/dist/lib/jquery/jquery-migrate.min.js"></script>
  <script src="/public/dist/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="/public/dist/lib/easing/easing.min.js"></script>
  <script src="/public/dist/lib/superfish/hoverIntent.js"></script>
  <script src="/public/dist/lib/superfish/superfish.min.js"></script>
  <script src="/public/dist/lib/wow/wow.min.js"></script>
  <script src="/public/dist/lib/waypoints/waypoints.min.js"></script>
  <script src="/public/dist/lib/counterup/counterup.min.js"></script>
  <script src="/public/dist/lib/owlcarousel/owl.carousel.min.js"></script>
  <script src="/public/dist/lib/isotope/isotope.pkgd.min.js"></script>
  <script src="/public/dist/lib/lightbox/js/lightbox.min.js"></script>
  <script src="/public/dist/lib/touchSwipe/jquery.touchSwipe.min.js"></script>
  <!-- Contact Form JavaScript File -->
  <script src="/public/dist/lib/contactform/contactform.js"></script>

  <!-- Template Main Javascript File -->
  <script src="/public/dist/lib/main.js"></script>

</body>
</html>
